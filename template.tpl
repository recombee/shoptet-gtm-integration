const injectScript = require('injectScript');
const callInWindow = require('callInWindow');
const log = require('logToConsole');
const queryPermission = require('queryPermission');
const copyFromWindow = require('copyFromWindow');
const copyFromDataLayer = require('copyFromDataLayer');
const setCookie = require('setCookie');
const getCookieValues = require('getCookieValues');
const makeNumber = require('makeNumber');

const helperLibrary = 'https://cdn.jsdelivr.net/gh/recombee/shoptet-gtm-integration/src/helper-library.min.js';
const recombeeLibrary = 'https://cdn.jsdelivr.net/gh/recombee/js-api-client/dist/recombee-api-client.min.js';

function getPageType (){
  return copyFromDataLayer('shoptet').pageType;
}

function getUserId() {
  var userId = copyFromDataLayer('shoptet').customer.guid; //shoptet logged user
  if (!userId) {
    log('getUID cookie =',getCookieValues('_rcmbid'));
    userId = getCookieValues('_rcmbid'); // returns LIST of cookie values with that name
    if ( !userId.length ){
      return undefined;
    }
    return userId;
  }
  log('getUID default =', userId);
  return userId;
}

function setUserId(){
  var options = {
    'max-age': 2629800  // one month expiration date for now
  };
  var userId = copyFromWindow('idGenerator');
  setCookie('_rcmbid', userId, options);
}

function isInitialized() {
  log("+++INIT++++CONTORL+++");
  if (copyFromWindow('idGenerator') == undefined){
    log('Library is not initialized');
    return false;
  }
  log('Library is initialized');
  return true;
}

function recombeeLibraryLoaded() {
  log('+++RECOMBEE SDK LOADED+++');
  injectScript(helperLibrary, helperLoaded, () => {
      log('ERROR: midLayer: Failed to inject');
      data.gtmOnFailure();
    });
}

function helperLoaded() {
  if ( callInWindow('initClient',data.databaseId,data.region,data.publicToken) == false ) {
    data.gtmOnFailure();
  }
  if (!getUserId()){
    setUserId();
  }
  log('+++MID LAYER LOADED+++');
  data.gtmOnSuccess();
}

switch (data.tagUseCase) {
  case "initialization":
    if (isInitialized()){
      log('callsuccess in initialization');
      data.gtmOnSuccess();
      break;
    }
    //initialization of tag
    if (queryPermission('inject_script', recombeeLibrary) && queryPermission('inject_script', helperLibrary)) {
      injectScript(recombeeLibrary, recombeeLibraryLoaded, () => {
      log('ERROR: recombeeLibrary: Failed to inject');
      data.gtmOnFailure();
    });
      
      log('INJECTSCRIPTS INJECTED');
    } else {
      log('ERROR: recombeeLibrary: Script load failed due to permissions mismatch.');
      data.gtmOnFailure();
    }
    break;
    
  case "detailView":
    log('-----detailView-----');
    if (getPageType() === 'productDetail') {
      var payload = {
        'userId' : getUserId().toString(),
        'itemId' : copyFromDataLayer('shoptet').product.code,
        'optional' : {
            'cascadeCreate' : data.cascadeCreate,
            'recommId': callInWindow('getRecombeeQueryParameter')
        }
      };
      log("detailView sending interactions");
      callInWindow('sendInteractions', payload, data.tagUseCase);
    }
    log('-----detailView-END-----');
    data.gtmOnSuccess();
    break;
    
  case "purchase":
    log('-----purchase-----');
    if (getPageType() === 'thankYou') {
      var items = copyFromDataLayer('transactionProducts');
      var userId = getUserId().toString();
      items.forEach(function (item) {
        var payload = {
          'userId' : userId,
          'itemId' : item.sku,
          'optional' : {
            'amount' : item.quantity,
            'price' : item.quantity * makeNumber(item.price),
            'cascadeCreate' : data.cascadeCreate,
            }
          };
        log("purchase sending interactions");
        callInWindow('sendInteractions', payload, data.tagUseCase);
      });
    }
    log('-----purchase--END-----');
    data.gtmOnSuccess();
    break;
    
  case "rating":
    log('-----rating-----');
    if (getPageType() === 'productDetail') {
      if (callInWindow("elementsPresent",".productRatingAction")) {
        var sendInteractionsFunction = copyFromWindow('sendInteractions');
        var payload = {
          'userId' : getUserId().toString(),
          'itemId' : copyFromDataLayer('shoptet').product.code,
          'optional' : {
            'cascadeCreate' : data.cascadeCreate,
            'recommId': callInWindow('getRecombeeQueryParameter')
          }
        };
        callInWindow("bindFunction",".productRatingAction",sendInteractionsFunction, payload, "click", data.tagUseCase);
      }
    }
    log('-----rating--END-----');
    break;
    
  case "cartAddition":
    log('-----cartAddition-----');
    log(copyFromDataLayer('ecommerce').add);
    var items = copyFromDataLayer('ecommerce').add;
    var userId = getUserId().toString();
    log('after datalayer pull');
    items.forEach( function (item) {
      var payload = {
        'userId' : userId,
        'itemId' : item.id,
        'optional' : {
          'amount' : item.quantity,
          'price' : item.quantity * makeNumber(item.price),
          'cascadeCreate' : data.cascadeCreate,
          'recommId': callInWindow('getRecombeeQueryParameter')
        }
      };
      log("cartAddition sending interactions");
      callInWindow('sendInteractions', payload, data.tagUseCase);
    });
    log('-----cartAddition-done-----');
    data.gtmOnSuccess();
    break;
    
  default:
    log("ERROR: Tag use case was not specified!");
    data.gtmOnFailure();
}


